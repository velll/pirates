{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [true, "never-prefix"],
        "ordered-imports": false,
        "quotemark": false,
        "object-literal-shorthand": [true, "never"],
        "semicolon": [true, "always", "ignore-interfaces"],
        "triple-equals": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "never",
                    "arrays": "never",
                    "functions": "never",
                    "typeLiterals": "ignore"
                },
                "esSpecCompliant": true
            }
        ]
    },
    "rulesDirectory": []
}